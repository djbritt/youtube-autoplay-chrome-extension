{
  "name": "YouTube Autoplay",
  "description": "Autoplay YouTube",
  "version": "1.0",
  "manifest_version": 2,

  "background": {
    "scripts": ["background.js"]
  },

  "permissions": [
    "tabs",
    "*://*.youtube.com/"
  ],

  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["contentScript.js"]
    }
  ]
}
